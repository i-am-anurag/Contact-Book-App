    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "laravel-mix": "^6.0.49",
    "moment": "^2.29.4",
    "mongoose": "^8.0.0",
    "nodemailer": "^6.9.13",
